{% endblock %}

{% block themeLayoutScripts %}
    <script src="{{ asset(METRONIC_LAYOUTS~'layout4/scripts/layout.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset(METRONIC_LAYOUTS~'layout4/scripts/demo.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset(METRONIC_LAYOUTS~'global/scripts/quick-sidebar.min.js') }}" type="text/javascript"></script>
{% endblock %}
