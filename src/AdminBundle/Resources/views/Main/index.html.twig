{% extends "PublicBundle:Global:baseIndex.html.twig" %}

{% block head %}
    {{ parent() }}
    <style>
        .user-login-5 .alert { margin-top:-5px; }
    </style>
{% endblock %}

{% block pageLevelPlugins %}
    <link rel="stylesheet" href="{{ asset(METRONIC_GLOBAL~'plugins/bootstrap-daterangepicker/daterangepicker.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset(METRONIC_GLOBAL~'plugins/morris/morris.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset(METRONIC_GLOBAL~'plugins/fullcalendar/fullcalendar.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset(METRONIC_GLOBAL~'plugins/jqvmap/jqvmap/jqvmap.css') }}" type="text/css" />
{% endblock %}

{% block themeLayoutStyles %}
    <link rel="stylesheet" href="{{ asset(METRONIC_LAYOUTS~'layout4/css/layout.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset(METRONIC_LAYOUTS~'layout4/css/themes/light.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset(METRONIC_LAYOUTS~'layout4/css/custom.min.css') }}" type="text/css" />
{% endblock %}
{% block content %}
    <!-- BEGIN HEADER -->
    <div class="page-header navbar navbar-fixed-top">
        <!-- BEGIN HEADER INNER -->
        <div class="page-header-inner ">
            <!-- BEGIN LOGO -->
            <div class="page-logo">
                <a href="index.html">
                    <img src="{{ asset(PUBLIC_IMAGES~'logo130_32.png') }}" alt="logo" class="logo-default" /> </a>
                <div class="menu-toggler sidebar-toggler">
                </div>
            </div>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse"> </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN PAGE TOP -->
            <div class="page-top">
                <!-- BEGIN TOP NAVIGATION MENU -->
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        <!-- BEGIN USER LOGIN DROPDOWN -->
                        <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
                        <li class="dropdown dropdown-user dropdown-dark">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <span class="username username-hide-on-mobile"> {{ app.user.username }} </span>
                                <!-- DOC: Do not remove below empty space(&nbsp;) as its purposely used -->
                                <img alt="" class="img-circle" src="{{ asset(UPLOADS_AVATARS~'avatar.png') }}" /> </a>
                            <ul class="dropdown-menu dropdown-menu-default">
                                <li>
                                    <a href="page_user_profile_1.html">
                                        <i class="icon-user"></i> 个人中心 </a>
                                </li>
                                <li class="divider"> </li>
                                <li>
                                    <a href="{{ path('logout') }}">
                                        <i class="icon-key"></i> 登出 </a>
                                </li>
                            </ul>
                        </li>
                        <!-- END USER LOGIN DROPDOWN -->
                    </ul>
                </div>
                <!-- END TOP NAVIGATION MENU -->
            </div>
            <!-- END PAGE TOP -->
        </div>
        <!-- END HEADER INNER -->
    </div>
    <!-- END HEADER -->
{% endblock %}

{% block javascript %}
    {{ parent() }}
{% endblock %}

{% block pageLevelPluginsScript %}
    <script src="{{ asset(METRONIC_GLOBAL~'plugins/moment.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset(METRONIC_GLOBAL~'plugins/bootstrap-daterangepicker/daterangepicker.min.js') }}" type="text/javascript"></script>
