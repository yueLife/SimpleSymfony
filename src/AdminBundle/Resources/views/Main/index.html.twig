    <script src="{{ asset(METRONIC_GLOBAL~'plugins/counterup/jquery.counterup.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset(METRONIC_GLOBAL~'plugins/amcharts/amcharts/amcharts.js') }}" type="text/javascript"></script>
